page {
    width: 100%;
    --td-cell-note-font-size: 26rpx;
    --td-cell-vertical-padding: 20rpx;
    --td-cell-title-font-size: 28rpx;
    --td-picker-confirm-color: #12977a;
}

.profile .title {
    font-size: 30rpx;
}

.profile .content {
    background: white;
}

.profile .content:last-child {
    margin-bottom: 200rpx;
}


.profile .content .username {
}

.profile .content .avatar {
    width: 80rpx;
    height: 80rpx;
}

.profile .content .avatar-button {
    background: #fff;
    height: auto;
    width: auto;
    margin: 0;
    display: flex;
    border: none;
    justify-content: center;
    align-items: center;
    padding: unset;
}
