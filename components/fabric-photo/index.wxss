page {
    background: #f2f2f2;
    height: 100vh;
    overflow: hidden;
    display: flex;
}
.container {
    display: flex;
    flex: 1;
    flex-direction: column;
    height: 100%;
}
.canvas-wrap {
    display: flex;
    margin: 10px;
    height: 50%;
}
.canvas {
    flex: 1;
    width: 100%;
    height: 100%;
    background: #fff;
}
.buttons {
    display: flex;
    justify-content: space-around;
    margin: 10px;
}
