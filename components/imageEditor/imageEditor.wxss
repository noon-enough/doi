.container {
  width: 100vw;
  height: 100vh;
  background-color: #000;
  display: flex;
  justify-content: stretch;
  align-items: center;
}

.layout {

  position: absolute;
  left: 10vw;
  right: 10vw;
  top: 10vw;
  bottom: 35vw;
  /* position: relative;
    width:100%;
    height:100%;
    margin:10vw; 
    flex: 1;*/
  background-color: black;
  display: flex;
  justify-content: center;
  align-items: center;
}

.stage {
  position: absolute;
  transition: all 0.3s linear;
}

.image {
  position: absolute;
  width: 100%;
  height: 100%;

}



.box {
  position: absolute;
  border: 4rpx solid grey;
  box-sizing: border-box;
  box-shadow: 0rpx 0rpx 0rpx 2000rpx rgba(0, 0, 0, 0.5);

}


.lt {
  position: absolute;
  height: 48rpx;
  width: 48rpx;
  left: -6rpx;
  top: -6rpx;
  border-left: 12rpx solid white;
  border-top: 12rpx solid white;
}

.lb {
  position: absolute;
  height: 48rpx;
  width: 48rpx;
  left: -6rpx;
  bottom: -6rpx;
  border-left: 12rpx solid white;
  border-bottom: 12rpx solid white;
}

.rt {
  position: absolute;
  height: 48rpx;
  width: 48rpx;
  right: -6rpx;
  top: -6rpx;
  border-right: 12rpx solid white;
  border-top: 12rpx solid white;
}

.rb {
  position: absolute;
  height: 48rpx;
  width: 48rpx;
  right: -6rpx;
  bottom: -6rpx;
  border-right: 12rpx solid white;
  border-bottom: 12rpx solid white;
}

.line-v {
  position: absolute;
  width: 2rpx;
  height: 100%;
  background-color: rgba(255, 255, 255, 0.5);
}

.line-h {
  height: 2rpx;
  position: absolute;
  width: 100%;
  background-color: rgba(255, 255, 255, 0.5);
}

/*  */
.toolbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 25vw;
  /* background-color: #333; */
  padding: 5vw;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;

}

.bar,
.sub-bar {

  display: flex;
  flex-direction: row;
  justify-content: space-around;

}

.sub-bar {
  justify-content: center;
  margin-bottom: 2.5vw;
}

.btn {
  position: relative;
  flex-grow: 1;
  /* border: 2rpx solid grey; */
  line-height: 80rpx;
  text-align: center;
  color: white;
  font-size: 32rpx;
}

.btn.active {
  color: #07c160;
}

.btn-color {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  position: relative;
  margin-right: 2.5vw;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}

.btn-color::before {
  background-color: var(--bg);

  content: '';
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
}

.btn-color.active {
  border: 6rpx solid #07c160;
}

.btn-size {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  position: relative;
  margin-right: 2.5vw;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}

.btn-size::before {
  background-color: #fff;

  content: '';
  width: var(--width);
  height: var(--width);
  border-radius: 50%;
}

.btn-size.active {
  border: 6rpx solid #07c160;
}

.slider {
  background: -webkit-linear-gradient(left, #f00 0%, #ff0 17%, #0f0 33%, #0ff 50%, #00f 67%, #f0f 83%, #f00 100%);
  margin: 0 5vw;
  border-radius: 2.5vw;
  width: 100%;

}

.btn-submit {
  background-color: #07c160;
  border-radius: 5px;
}